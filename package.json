{
  "name": "landing",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel \"test:watch -- --reporter min \" open:src lint:watch",
    "dev": "npm run open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "npm run lint:prebuild --silent",
    "lint:prebuild": "esw webpack.config.* src tools test features --color",
    "lint:fix": "npm run lint:prebuild --silent -- --fix",
    "lint:watch": "npm run lint:prebuild -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint:prebuild && npm run test:prebuild",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "npm run test:prebuild --silent",
    "test:prebuild": "NODE_PATH=src mocha tools/testSetup.js \"./test/**/*.js\"",
    "test:cucumber": "rimraf ./screenshots && NODE_PATH=src wdio wdio.conf.js",
    "test:coverage": "NODE_PATH=src babel-node ./node_modules/.bin/isparta cover _mocha -- --require ./tools/testSetup.js \"./test/**/*.js\" && open coverage/lcov-report/index.html",
    "test:watch": "npm run test:prebuild -- --watch"
  },
  "author": "≈Åukasz",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-fa": "4.1.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "superagent": "3.4.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
